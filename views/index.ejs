<!DOCTYPE html>
<html>
  <head>
    <%- include('partials/head.ejs', { title: 'DOU\'s images' }) %>
  </head>
  <body>
    <div class="container">
      <div class="row pt-3">
        <h3>Images</h3>

        <br />
        <div class="row">
          <% images.forEach(function(image) {%>
          <div class="col-12">
            <p><%= image %></p>
            <img
              src="<%= image %>"
              class="img-fluid"
              style="width: 100%"
              alt="<%= image %>"
            />
          </div>
          <% }) %>
        </div>
      </div>
    </div>

    <%- include('partials/footer') %>
  </body>
</html>
